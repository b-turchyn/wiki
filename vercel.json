{
  "github": {
    "silent": true
  },
  "rewrites": [
    {
      "source": "/stats/js/script.outbound-links.js",
      "destination": "https://plausible.io/js/script.outbound-links.js"
    },
    {
      "source": "/stats/api/event",
      "destination": "https://plausible.io/api/event"
    }
  ],
  "redirects": [
    {
      "source": "/books/50-days-of-writing(/?)",
      "destination": "/books/days-of-writing/"
    },
    {
      "source": "/computers/file-sharing(/?)",
      "destination": "/privacy/file-transfers/"
    },
    {
      "source": "/design/color(/?)",
      "destination": "/design/colour/"
    },
    {
      "source": "/productivity/automation(/?)",
      "destination": "/automation/"
    },
    {
      "source": "/productivity/email(/?)",
      "destination": "/computers/email/"
    },
    {
      "source": "/devops/bash(/?)",
      "destination": "/programming/languages/bash/"
    },
    {
      "source": "/programming/flutter(/?)",
      "destination": "/programming/frameworks/flutter/"
    },
    {
      "source": "/technology/keyboards(/?)",
      "destination": "/keyboards/"
    }
  ]
}
